; LauncherApp.pkg
;
;Language - standard language definitions
&EN

; standard SIS file header
#{"%project-name%"},(0x%uid%),1,2,0, TYPE=SA

;Localised Vendor name
%%{"%vendor-name%"}

;Unique Vendor name
:"%vendor-name%"

;Supports S60 5th Edition
[0x1028315F], 0, 0, 0, {"Series60ProductID"}

